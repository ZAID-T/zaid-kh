  .btn-mktg:hover .arrow-symbol-mktg, .btn-mktg:focus .arrow-symbol-mktg, .link-mktg:hover .arrow-symbol-mktg, .link-mktg:focus .arrow-symbol-mktg, .arrow-target-mktg:hover .arrow-symbol-mktg, .arrow-target-mktg:focus .arrow-symbol-mktg { transform: translateX(4px); }
