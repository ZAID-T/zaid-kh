.link-mktg.arrow-target-mktg .arrow-symbol-mktg { margin-left: -0.25em; }

.hover-grow-mktg { transition: transform 0.4s cubic-bezier(0.16, 1, 0.3, 1) 0s; }

.hover-grow-mktg:hover { transform: scale3d(1.025, 1.025, 1.025); }

.btn-mktg .octicon, .link-mktg .octicon, .arrow-target-mktg .octicon { width: 1em; height: 1em; }

.btn-mktg .arrow-symbol-mktg, .link-mktg .arrow-symbol-mktg, .arrow-target-mktg .arrow-symbol-mktg { transition: transform 0.2s ease 0s; transform: translateX(0px); }

.btn-mktg .arrow-symbol-mktg path:last-child, .link-mktg .arrow-symbol-mktg path:last-child, .arrow-target-mktg .arrow-symbol-mktg path:last-child { stroke-dasharray: 10; stroke-dashoffset: 10; transition: stroke-dashoffset 0.2s ease 0s; }

